---
const response = await fetch('https://jsonplaceholder.typicode.com/users');
const data = await response.json();
console.log(data);
---

<table>
  <th>Name</th>
  <th>Username</th>
  <th>Email</th>
  {
    data.map((item) => (
        <tr>
            <td>{item.name}</td>
            <td>{item.username}</td>
            <td>{item.email}</td>

        </tr>
    ))
  }
</table>

<style>
    table {
        width: 100%;
        border: 2px solid black;
    }
    td {
        background-color: aquamarine;
        border-radius: 20%;
        padding: 10px;
    }
</style>